#include <stdio.h>

int main()
{
    char ch;
    printf("enter a chracter");
    scanf("%c" ,&ch);
    if(ch>='A' && ch<='Z')
    {
        printf("the entered character is uppercase");
    }
    else if(ch>='a' && ch<='z')
    {
        printf("the entered chracter is lowercase");
    }
    else if(ch>='0' && ch<='9')
    {
        printf("the entered character is a digit");
    }
    else{
        printf("the entered character is a special character");
    }
    return 0;
}
