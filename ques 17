#include <stdio.h>
#include<math.h>

int main()
{
    float a,b,c,d,root1,root2;
    printf("enter the coefficients a,b,c:");
    scanf("%f %f %f", &a, &b, &c);
    d=(b*b)-4*a*c;
    if(d>0)
    {
        root1=(-(b*b)+ sqrt(d))/(2*a);
        root2=(-(b*b)- sqrt(d))/(2*a);
        printf("root1 and root2 are:%f %f \n",root1,root2);
        printf("roots are real and distinct\n");
    }
    else if(d==0)
    {
        root1=root2=-b/(2*a);
        printf("roo1 and root2 are:%f %f \n", root1,root2);
        printf("roots are real and equal\n");
    }
    else{
        printf("roots are imaginary\n");
    }
    return 0;
}
